#include <iostream>
using namespace std;
// 2576. 홀수

int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);
    int num;
    int sum = 0, min = 100;
    for(int i = 0; i < 7; i++) {
        cin >> num;
        if(num % 2 == 1) {
            sum += num;
            if(num < min) min = num;
        }
    }
    if(sum == 0) cout << -1;
    else cout << sum << "\n" << min;
}
